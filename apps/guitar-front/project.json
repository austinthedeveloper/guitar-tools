{
  "name": "guitar-front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/guitar-front/src",
  "prefix": "guitar",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": {
          "base": "dist/apps/guitar-front"
        },
        "index": "apps/guitar-front/src/index.html",
        "polyfills": [
          "apps/guitar-front/src/polyfills.ts"
        ],
        "tsConfig": "apps/guitar-front/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/guitar-front/src/favicon.ico",
          "apps/guitar-front/src/staticwebapp.config.json",
          "apps/guitar-front/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/guitar-front/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "./node_modules",
            "./libs"
          ]
        },
        "scripts": [],
        "browser": "apps/guitar-front/src/main.ts"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/guitar-front/src/environments/environment.ts",
              "with": "apps/guitar-front/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "guitar-front:build:production"
        },
        "development": {
          "buildTarget": "guitar-front:build:development"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "guitar-front:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/apps/guitar-front"
      ],
      "options": {
        "jestConfig": "apps/guitar-front/jest.config.ts"
      }
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": [
        "{workspaceRoot}/dist/.playwright/apps/guitar-front"
      ],
      "options": {
        "config": "apps/guitar-front/playwright.config.ts"
      }
    }
  }
}
