{
  "version": 1,
  "projects": {
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/data/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data"],
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "drum-front": {
      "projectType": "application",
      "root": "apps/drum-front",
      "sourceRoot": "apps/drum-front/src",
      "prefix": "guitar",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/drum-front",
            "index": "apps/drum-front/src/index.html",
            "main": "apps/drum-front/src/main.ts",
            "polyfills": "apps/drum-front/src/polyfills.ts",
            "tsConfig": "apps/drum-front/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/drum-front/src/favicon.ico",
              "apps/drum-front/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/drum-front/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/drum-front/src/environments/environment.ts",
                  "with": "apps/drum-front/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "drum-front:build:production"
            },
            "development": {
              "browserTarget": "drum-front:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "drum-front:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/drum-front/src/**/*.ts",
              "apps/drum-front/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/drum-front"],
          "options": {
            "jestConfig": "apps/drum-front/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "drum-front-e2e": {
      "root": "apps/drum-front-e2e",
      "sourceRoot": "apps/drum-front-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/drum-front-e2e/cypress.json",
            "devServerTarget": "drum-front:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "drum-front:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/drum-front-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "drums": {
      "projectType": "library",
      "root": "libs/drums",
      "sourceRoot": "libs/drums/src",
      "prefix": "guitar",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/drums"],
          "options": {
            "jestConfig": "libs/drums/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/drums/src/**/*.ts",
              "libs/drums/src/**/*.html"
            ]
          }
        }
      }
    },
    "guitar-front": {
