            "jestConfig": "apps/guitar-front/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "guitar-front-e2e": {
      "root": "apps/guitar-front-e2e",
      "sourceRoot": "apps/guitar-front-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
